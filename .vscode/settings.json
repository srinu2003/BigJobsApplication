{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.sf": true,
    "**/.sfdx": true
  },
  "xml.preferences.showSchemaDocumentationType": "none",
  "salesforcedx-vscode-core.show-cli-success-msg": false,
  "salesforce.einsteinForDevelopers.enable": true,
  "salesforce.einsteinForDevelopers.enableAutocompletions": false,
  "xml.fileAssociations": [
    {
      "pattern": "**/*.js-meta.xml",
      "systemId": "${workspaceFolder}/.vscode/sfdc-lwc-meta.xsd"
    }
  ]
}
